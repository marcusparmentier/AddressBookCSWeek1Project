<h1>Contact List</h1>
<ul>
  @foreach(var contact in Model)
  {
      <li><a href='/contact/@contact.GetId()'>@contact.GetName()</a></li>
  }
</ul>
<br>

<h3><a href='/contact/form'>Add a new contact</a></h3>
<br>

<form action="/contacts/clear" method="post">
  <button type="submit" name="button">Clear all contacts</button>
</form>
